# API Configuration
# Point this to your Next.js backend URL
EXPO_PUBLIC_API_URL=http://localhost:3000

# Google OAuth Configuration
# Get these from Google Cloud Console (https://console.cloud.google.com/)
# Create OAuth 2.0 credentials for Web, Android, and iOS

# Web Client ID (required) - should match GOOGLE_CLIENT_ID in backend
EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID=your_google_web_client_id_here

# Android Client ID (optional - only if deploying to Android)
EXPO_PUBLIC_GOOGLE_ANDROID_CLIENT_ID=your_google_android_client_id_here

# iOS Client ID (optional - only if deploying to iOS)
EXPO_PUBLIC_GOOGLE_IOS_CLIENT_ID=your_google_ios_client_id_here
